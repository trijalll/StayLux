              </div>
              <div class="card__center__tittle">
                  <p>New York City</p>
              </div>
          </div>
          </div>
          <div class="card__right">
            <%- include('../include/cards.ejs') %> 
            <%- include('../include/cards.ejs') %>
          </div>
        </div>
        <div class="part2__button">
          <button>View all 25+ cities</button>
        </div>
      </div>

